    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 5",
    "Android >= 4"
  ],
  "standard": {
    "ignore": [
      "/build/",
      "/config/",
      "/dist/",
      "/node_modules/",
      "/src/assets/",
      "/static/"
    ]
  }
}
